<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="650.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.controllers.ResultController">
   <children>
      <Label layoutX="188.0" layoutY="133.0" prefHeight="58.0" prefWidth="162.0" text="PLAYER 1">
         <font>
            <Font name="Calibri" size="40.0" />
         </font>
      </Label>
      <Label layoutX="155.0" layoutY="254.0" prefHeight="58.0" prefWidth="139.0" text="WON!" textFill="#0ae13c">
         <font>
            <Font name="Calibri Bold" size="50.0" />
         </font>
      </Label>
      <ImageView fitHeight="67.0" fitWidth="86.0" layoutX="91.0" layoutY="129.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/imageX.png" />
         </image>
      </ImageView>
      <Button fx:id="playAgainButton" layoutX="75.0" layoutY="409.0" mnemonicParsing="false" onAction="#switchToGamePlay" prefHeight="42.0" prefWidth="300.0" styleClass="button-gradient" stylesheets="@../stylesheets/main.css" text="PLAY  AGAIN" textFill="WHITE">
         <font>
            <Font name="Berlin Sans FB" size="18.0" />
         </font>
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
      </Button>
      <Button fx:id="backToHomeButton" layoutX="75.0" layoutY="486.0" mnemonicParsing="false" onAction="#switchToHome" prefHeight="42.0" prefWidth="300.0" styleClass="button-gradient" stylesheets="@../stylesheets/main.css" text="HOME" textFill="WHITE">
         <font>
            <Font name="Berlin Sans FB" size="18.0" />
         </font>
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
      </Button>
   </children>
</AnchorPane>
